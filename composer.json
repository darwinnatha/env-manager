{
    "name": "darwinnatha/env-manager",
    "description": "manage easily other ways env entries",
    "type": "library",
    "keywords": [
        "env",
        "manager"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Darwinnatha\\EnvManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Darwinnatha\\EnvManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Darwinnatha\\EnvManager\\EnvManagerServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Darwin Nathan",
            "email": "fotsodarwin@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/console": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.18",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.2"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
